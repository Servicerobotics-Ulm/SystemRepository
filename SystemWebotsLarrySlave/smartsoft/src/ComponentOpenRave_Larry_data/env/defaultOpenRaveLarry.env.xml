<!--Environment consisting of the floor the larry-robot-base including a separate collision model and a view perspective onto the scene. The manipulator is loaded separately.-->  
<Environment>
    <camtrans>-0.387017 1.534908 2.202861</camtrans>
    <camrotationaxis>-0.233246 0.898392 -0.372140 166.532253</camrotationaxis>
    <camfocal>2.381189</camfocal>

<!--*********************Floor,Table and RobotBase********************************-->  
	
	<KinBody name="floor">
		<Body name ="floor" type="static">
			<Translation>0.0 0.0 -0.2005</Translation>
			<Geom type="box">
				<extents>2 2 0.0005</extents>
				<diffuseColor>0.6 0.6 0.6</diffuseColor>
				<ambientColor>0.6 0.6 0.6</ambientColor>
			</Geom>
		</Body>
	</KinBody> 

